{
	"name": "resting-squirrel",
	"version": "2.28.2",
	"author": "Lukas Macuda <lukas.macuda@gmail.com>",
	"main": "./dist",
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "./node_modules/.bin/mocha ./__tests__ --recursive --require @babel/register --timeout 5000 --exit",
		"create_index": "run-script-os",
		"create_index:win32": "echo module.exports = require('./dist'); > index.js",
		"create_index:linux:darwin": "echo \"module.exports = require('./dist');\" > index.js",
		"build_es6": "./node_modules/.bin/babel ./src --out-dir ./dist --copy-files && npm run create_index",
		"clear": "rm -rf ./dist && rm -f ./index.js",
		"prepublishOnly": "npm run build_es6 && npm run bundle:doc",
		"postpublish": "npm run clear",
		"test_app": "npm run clear && npm run build_es6 && node ./test.js",
		"preversion": "git branch %npm_package_version% && git checkout %npm_package_version%",
		"bundle:doc": "./node_modules/.bin/webpack --config ./react/webpack.config.js --progress",
		"bundle:doc:dev": "./node_modules/.bin/webpack --config ./react/webpack.config.dev.js --progress --watch"
	},
	"license": "MIT",
	"engines": {
		"node": ">=4"
	},
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"@types/express": "4.17.0",
		"@types/express-serve-static-core": "4.16.7",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"express": "^4.17.1",
		"http-smart-error": "^4.0.1",
		"lodash": "^4.17.21",
		"route-parser": "0.0.5",
		"runtime-type": "^4.0.1",
		"smart-error": "^3.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zabkwak/resting-squirrel.git"
	},
	"bugs": {
		"url": "https://github.com/zabkwak/resting-squirrel/issues"
	},
	"homepage": "https://github.com/zabkwak/resting-squirrel#readme",
	"description": "Simple REST server with json input data.",
	"keywords": [
		"rest",
		"express"
	],
	"devDependencies": {
		"@babel/cli": "^7.15.4",
		"@babel/core": "^7.15.5",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"@babel/preset-env": "^7.15.4",
		"@babel/register": "^7.15.3",
		"@material-ui/core": "^4.12.3",
		"@material-ui/icons": "^4.11.2",
		"@types/camelcase": "^5.2.0",
		"@types/classnames": "^2.3.1",
		"@types/prop-types": "^15.7.4",
		"@types/react": "^16.14.15",
		"@types/react-copy-to-clipboard": "^4.3.0",
		"@types/react-dom": "^16.9.14",
		"@types/uniqid": "^5.3.1",
		"camelcase": "^8.0.0",
		"chai": "^4.5.0",
		"classnames": "^2.3.1",
		"css-loader": "^7.1.2",
		"es7-object-polyfill": "^1.0.1",
		"html-to-react": "^1.4.5",
		"mocha": "^10.8.2",
		"polyfill-array-includes": "^2.0.0",
		"promise-polyfill": "^8.2.0",
		"prop-types": "^15.7.2",
		"querystring": "^0.2.1",
		"react": "^16.14.0",
		"react-copy-to-clipboard": "^5.0.4",
		"react-dom": "^16.14.0",
		"react-json-view": "^1.21.3",
		"request": "^2.88.2",
		"run-script-os-fix": "^1.0.4",
		"sass": "^1.81.0",
		"sass-loader": "^16.0.3",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.6.3",
		"uniqid": "^5.4.0",
		"webpack": "^5.96.1",
		"webpack-cli": "^5.1.4",
		"whatwg-fetch": "^3.6.2"
	}
}
