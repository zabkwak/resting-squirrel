{
  "name": "resting-squirrel",
  "version": "2.8.11",
  "author": "Lukas Macuda <lukas.macuda@gmail.com>",
  "main": "index.js",
  "types": "./dist/resting-squirrel.d.ts",
  "scripts": {
    "test": "mocha ./__tests__ --recursive --compilers js:babel-core/register --timeout 5000",
    "create_index": "run-script-os",
    "create_index:win32": "echo module.exports = require('./dist'); > index.js",
    "create_index:linux:darwin": "echo \"module.exports = require('./dist');\" > index.js",
    "build_es6": "./node_modules/.bin/babel ./src --out-dir ./dist --copy-files && npm run create_index",
    "clear": "rm -rf ./dist && rm -f ./index.js",
    "prepublish": "npm run build_es6",
    "postpublish": "npm run clear",
    "test_app": "npm run build_es6 && node ./test.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.14.2",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "http-smart-error": "^1.1.1",
    "route-parser": "0.0.5",
    "runtime-type": "^1.2.1",
    "smart-error": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zabkwak/resting-squirrel.git"
  },
  "bugs": {
    "url": "https://github.com/zabkwak/resting-squirrel/issues"
  },
  "homepage": "https://github.com/zabkwak/resting-squirrel#readme",
  "description": "Simple REST server with json input data.",
  "keywords": [
    "rest",
    "express"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "request": "^2.81.0",
    "run-script-os-fix": "^1.0.4"
  }
}
